{
  "name": "bot_webhook_workplace",
  "version": "1.0.0",
  "description": "Capta as mensagens",
  "main": "index.js",
  "config": {
    "function_name": "app",
    "lambda_name": "bot_webhook_workplace",
    "region": "us-east1",
    "gcp_project": "rd-faq",
    "runtime": "nodejs16",
    "memory": "128MB"
  },
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm test -- --watch",
    "test:integration": "npm test -- --watch --runInBand",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "pkg:build": "run-s pkg:clean pkg:lint pkg:tsc pkg:copy-pkg",
    "pkg:clean": "rimraf \"dist/*\"",
    "pkg:copy-pkg": "copyfiles package.json dist/src",
    "pkg:lint": "tslint --project tsconfig.json",
    "pkg:tsc": "tsc",
    "exec:script": "./script.js",
    "exec:local_http": "node dist/src/index.js",
    "exec:local_gcp": "functions-framework --target=${npm_package_config_function_name} --source=dist/src",
    "exec:dev": "gcloud functions deploy ${npm_package_config_lambda_name}_dev --region=${npm_package_config_region} --allow-unauthenticated --entry-point=${npm_package_config_function_name} --memory=${npm_package_config_memory} --runtime=${npm_package_config_runtime} --env-vars-file=./config/.env.dev.yaml --build-env-vars-file=./config/.env.dev.yaml --trigger-http --project=${npm_package_config_gcp_project} --source=dist/src",
    "exec:qa": "gcloud functions deploy ${npm_package_config_lambda_name}_qa --region=${npm_package_config_region} --allow-unauthenticated --entry-point=${npm_package_config_function_name} --memory=${npm_package_config_memory} --runtime=${npm_package_config_runtime} --env-vars-file=./config/.env.qa.yaml --build-env-vars-file=./config/.env.qa.yaml --trigger-http --project=${npm_package_config_gcp_project} --source=dist/src",
    "exec:prod": "gcloud functions deploy ${npm_package_config_lambda_name}_prod --region=${npm_package_config_region} --allow-unauthenticated --entry-point=${npm_package_config_function_name} --memory=${npm_package_config_memory} --runtime=${npm_package_config_runtime} --trigger-http --project=${npm_package_config_gcp_project} --source=dist/src --env-vars-file=./config/.env.prod.yaml --build-env-vars-file=./config/.env.prod.yaml",
    "deploy:local_http": "run-s pkg:build exec:local_http",
    "deploy:local_gcp": "run-s test:ci pkg:build exec:local_gcp",
    "deploy:dev": "run-s test:ci pkg:build exec:dev",
    "deploy:qa": "run-s test:ci pkg:build exec:qa",
    "deploy:prod": "run-s test:ci pkg:build exec:prod",
    "gcloud:info": "gcloud info --run-diagnostics"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "express": "^4.17.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.0.0",
    "axios": "^0.26.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "flatted": "^3.2.5",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0"
  }
}
